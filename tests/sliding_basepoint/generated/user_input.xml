<?xml version="1.0" ?>
<FIT>
    <EXPERIMENT>
        <FILENAME>
            <E> FILENAME_DATA = tests/sliding_basepoint/inputs/sliding_basepoint_data.csv </E>
        </FILENAME>
    </EXPERIMENT>
    <MODEL>
        <TRAINABLE_PARAMETERS>
            <P> N_TRAINABLE_PARAMETERS = 3 </P>
        </TRAINABLE_PARAMETERS>
        <TRAINABLE_PARAMETER_DESCRIPTION>
            <PARAM>
                <P> PARAMETER_NAME= c2 </P>
                <P> MIN_VAL=1E5 </P>
                <P> MAX_VAL=4E5 </P>
                <P> LOGSCALE= Y </P>
            </PARAM>
            <PARAM>
                <P> PARAMETER_NAME= Dk </P>
                <P> MIN_VAL=0.5 </P>
                <P> MAX_VAL=10 </P>
                <P> LOGSCALE= N </P>
            </PARAM>
            <PARAM>
                <P> PARAMETER_NAME= Dc </P>
                <P> MIN_VAL=0.1 </P>
                <P> MAX_VAL=1 </P>
                <P> LOGSCALE= N </P>
            </PARAM>
        </TRAINABLE_PARAMETER_DESCRIPTION>
        <FIXED_PARAM_DESCRIPTION>
            <PARAM>
                <P> NAME = m1 </P>
                <P> VALUE= 1916 </P>
            </PARAM>
            <PARAM>
                <P> NAME = m2 </P>
                <P> VALUE= 24.9 </P>
            </PARAM>
            <PARAM>
                <P> NAME = vf </P>
                <P> VALUE= 0.1 </P>
            </PARAM>
        </FIXED_PARAM_DESCRIPTION>
        <INTEGRATED_SYSTEM_DESCRIPTION>
            <VAR>
                <P> NAME = x1 </P>
                <P> INIT_VAL = 0 </P>
            </VAR>
            <VAR>
                <P> NAME = x2 </P>
                <P> INIT_VAL = 0 </P>
            </VAR>
            <VAR>
                <P> NAME = v1 </P>
                <P> INIT_VAL = 22.1 </P>
            </VAR>
            <VAR>
                <P> NAME = v2 </P>
                <P> INIT_VAL = 0 </P>
            </VAR>
            <VAR>
                <P> NAME = k </P>
                <P> INIT_VAL = 8.45E6 </P>
            </VAR>
            <VAR>
                <P> NAME = c1 </P>
                <P> INIT_VAL = 0 </P>
            </VAR>
        </INTEGRATED_SYSTEM_DESCRIPTION>
    </MODEL>
    <POPULATION_OPT>
        <SETTINGS>
            <P> NUM_PARTICLES = 500 </P>
            <P> NUM_ITERS = 10 </P>
            <P> PROCESSORS = 8 </P>
        </SETTINGS>
    </POPULATION_OPT>
    <GRADIENT_OPT>
        <SETTINGS>
            <P> NUM_ITERS = 10 </P>
            <P> STEPSIZE_RTOL = 1e-3,1e-3,1e-3,1e-3,1e-5,1e-5 </P>
            <P> STEPSIZE_ATOL = 1e-5,1e-5,1e-3,1e-3,1e-2,1e-2 </P>
            <P> INITIAL_TIMESTEP = 1e-9 </P>
            <P> MAX_STEPS = 10000 </P>
            <P> INIT_VALUE_LR = 1e-4 </P>
            <P> END_VALUE_LR = 1e-5 </P>
            <P> TRANSITION_STEPS_LR = 2000 </P>
            <P> DECAY_RATE_LR = 0.9 </P>
        </SETTINGS>
    </GRADIENT_OPT>
    <PLOTTING_INFO>
        <P> WRITE_RESULTS = Y </P>
    </PLOTTING_INFO>
</FIT>
